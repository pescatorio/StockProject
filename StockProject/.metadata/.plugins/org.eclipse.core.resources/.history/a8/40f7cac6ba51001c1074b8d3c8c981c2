package VO;

public class ProductVO {

	private int product_id;
	private String stock;
	private String price_number;
	private int mid;
	private String product_name;
	private String status;
	
	public int getProduct_id() {
		return product_id;
	}

	public void setProduct_id(int product_id) {
		this.product_id = product_id;
	}

	public String getStock() {
		return stock;
	}

	public void setStock(String stock) {
		this.stock = stock;
	}

	public String getPrice_number() {
		return price_number;
	}

	public void setPrice_number(String price_number) {
		this.price_number = price_number;
	}

	public int getMid() {
		return mid;
	}

	public void setMid(int mid) {
		this.mid = mid;
	}

	public String getProduct_name() {
		return product_name;
	}

	public void setProduct_name(String product_name) {
		this.product_name = product_name;
	}

	public ProductVO(int product_id, String stock, String price_number,
			int mid, String product_name) {

		super();
		this.product_id = product_id;
		this.stock = stock;
		this.price_number = price_number;
		this.mid = mid;
		this.product_name = product_name;
	}

	@Override
	public String toString() {
		return "ProductVO [product_id=" + product_id + ", stock=" + stock + "]";
	}
	
	public ProductVO(){}//기본생성자
// 13 버전입니다.
}
